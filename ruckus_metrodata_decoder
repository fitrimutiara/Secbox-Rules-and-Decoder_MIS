<!-- Parent Decoder - Matches the actual log format with syslog prefix -->
<decoder name="ruckus">
  <prematch>Metrodata-Wireless Core: @@</prematch>
</decoder>

<!-- Event ID and Action Decoder -->
<decoder name="ruckus">
  <parent>ruckus</parent>
  <regex type="pcre2">@@(\d+),([^,]+),</regex>
  <order>event_id,action</order>
</decoder>

